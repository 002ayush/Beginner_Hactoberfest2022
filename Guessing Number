import random
a=(random.randint(1,100))
count=1
higher = 100
lower = 1
print(a)
while(1):
    b=input()
    if b=='G':
        higher = a-1
        a=(random.randint(lower,higher))
        print(a)
        count+=1
    elif b=='L':
        lower = a+1
        a=(random.randint(lower,higher))
        print(a)
        count+=1
    elif b=='C':
        print('Your no.is',a )
        print("I have guessed it in",count,'attempts')
        break
